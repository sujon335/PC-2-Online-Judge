#include<stdio.h>#include<math.h>double line(double,double,double,double);double area(double,double,double,double);int main(){double a,b,c,d,e,f;double m,n,o,p,j,pp,qq,rr;printf("Input First Co-ordinate:\n>(a,b)=");scanf("%lf%lf",&a,&b);printf("Input First Co-ordinate:\n>(c,d)=");scanf("%lf%lf",&c,&d);printf("Input First Co-ordinate:\n>(e,f)=");scanf("%lf%lf",&e,&f);pp=line(a,b,c,d);qq=line(c,d,e,f);rr=line(e,f,a,b);m=sqrt(pp);n=sqrt(qq);o=sqrt(rr);printf("\n1st line length of the triangle is=%lf\n",m);printf("2nd line length of the triangle is=%lf\n",n);printf("3rd line length of the triangle is=%lf\n",o);p=m+n+o;j=sqrt(area(m,n,o,p));printf("AREA of the TRIANGLE=%lf\n",j);}double line(double x,double y,double z,double k){double t;t=(((x-z)*(x-z))+((y-k)*(y-k)));return (t);}double area(double q,double r,double s,double h){double l;l=(h*(h-q)*(h-r)*(h-s));return(l);}